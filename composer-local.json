{
  "name": "planet4-netherlands/planet4-netherlands",
  "description": "Greenpeace P4 Netherlands",
  "license": "MIT",
  "require": {
    "greenpeace/planet4-child-theme-netherlands": "4.*",
    "greenpeace/planet4-gpnl-plugin-gutenberg-blocks": "2.*",
    "wpackagist-plugin/wordpress-seo": "18.1.*"
  },
  "scripts": {
    "core:language": "wp language core install nl_NL; wp language core activate nl_NL",
    "install:plugin-yoast": "wp plugin install --activate https://storage.googleapis.com/planet4-3rdparty-plugins/wordpress-seo-premium-18.0.zip",
    "site:custom": [
      "@core:language",
      "@install:plugin-yoast"
    ]
  }
}
