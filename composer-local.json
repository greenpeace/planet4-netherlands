{
  "name": "planet4-netherlands/planet4-netherlands",
  "description": "Greenpeace P4 Netherlands",
  "license": "MIT",
  "require": {
    "greenpeace/planet4-child-theme-netherlands": "dev-develop",
    "greenpeace/planet4-gpnl-plugin-blocks": "dev-develop",
    "greenpeace/planet4-gpnl-plugin-gutenberg-blocks": "dev-feature/apiconnector",
    "wikimedia/composer-merge-plugin": "1.4.1"
  },
    "merge-plugin": {
      "require": "vendor/plugins/planet4-gpnl-plugin-gutenberg-blocks/composer.json",
      "recurse": true,
      "replace": true,
      "ignore-duplicates": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": true
    },
  "scripts": {
    "core:language": "wp language core install nl_NL; wp language core activate nl_NL",
    "site:custom": [
      "@core:language"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
